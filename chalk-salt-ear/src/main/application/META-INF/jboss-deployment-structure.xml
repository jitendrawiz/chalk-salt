<?xml version="1.0" encoding="UTF-8"?>

<jboss-deployment-structure> 
    <ear-subdeployments-isolated>false</ear-subdeployments-isolated>
    <deployment>
        <dependencies>
            <module name="com.mysql" export="true" />
            <module name="org.codehaus.jackson.jackson-mapper-asl" export="true" />
            <module name="org.slf4j" export="true" />
            <module name="org.slf4j.jcl-over-slf4j" export="true" />
            <module name="com.sun.xml.bind" />
            <module name="org.jboss.as.controller-client" />
            <module name="org.jboss.dmr" />
            <module name="org.apache.xerces" export="true"/>
            <module name="org.jboss.resteasy.resteasy-jaxrs" export="true"/>
        </dependencies>
    </deployment>
</jboss-deployment-structure>
